{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "es6",
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": true,
    "incremental": true,
    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": false
  },
  "exclude": ["node_modules"]
}
